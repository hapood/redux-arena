{
  "presets": ["react"],
  "env": {
    "cjs": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ]
      ],
      "plugins": [
        "syntax-dynamic-import",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": [
        "syntax-dynamic-import",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
