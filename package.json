{
  "name": "redux-arena",
  "version": "0.6.3",
  "description":
    "Bundling reducers, actions, saga and react-component when using Redux",
  "main": "index.js",
  "scripts": {
    "build":
      "npm run build:babel && npm run build:copy-files && npm run build:es",
    "build:es":
      " cross-env BABEL_ENV=es babel ./src/index.js --out-file ./build/index.es.js",
    "build:babel":
      "cross-env NODE_ENV=release babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "postpublish": "npm run build && cd build && npm publish",
    "test": "karma start test/karma.conf.js",
    "test-travis": "karma start test/karma.conf.js --browsers Firefox --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hapood/redux-arena.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "redux-saga",
    "context-switch"
  ],
  "author": "Hapood Wang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hapood/redux-arena/issues"
  },
  "homepage": "https://github.com/hapood/redux-arena#readme",
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "fs-extra": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webpack": "^2.0.4",
    "loglevel": "^1.4.1",
    "mocha": "^3.5.0",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-chart-monitor": "^1.6.1",
    "redux-devtools-diff-monitor": "^5.0.5",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
