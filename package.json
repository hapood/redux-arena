{
  "name": "redux-arena",
  "version": "0.7.1",
  "description": "Bundling reducers, actions, saga and react-component when using Redux",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:**",
    "build:cjs": "cross-env BABEL_ENV=cjs babel ./src --out-dir ./build/lib --ignore spec.js",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./build/es --ignore spec.js",
    "build:copy-files": "cross-env BABEL_ENV=cjs babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "postpublish": "npm run build && cd build && npm publish",
    "test": "cross-env BABEL_ENV=es karma start test/karma.conf.js",
    "test-travis": "cross-env BABEL_ENV=es karma start test/karma.conf.js --browsers Firefox --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hapood/redux-arena.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "redux-saga",
    "context-switch"
  ],
  "author": "Hapood Wang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hapood/redux-arena/issues"
  },
  "homepage": "https://github.com/hapood/redux-arena#readme",
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "prop-types": "^15.6.0",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "fs-extra": "^4.0.2",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webpack": "^2.0.4",
    "loglevel": "^1.5.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "16",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
