{
  "version": "2.5",
  "compilerOptions": {
    "target": "es5",
    "outDir": "lib",
    "sourceMap": false,
    "declaration": true,
    "module": "commonjs",
    "removeComments": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "jsx": "react"
  },
  "exclude": ["node_modules", "example"]
}
